<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>

<AnchorPane xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Pane prefHeight="200.0" prefWidth="300.0">
         <children>
            <Button fx:id="btnLogin" layoutX="30.0" layoutY="151.0" mnemonicParsing="false" text="Login" />
            <Label fx:id="lblUsuario" layoutX="23.0" layoutY="67.0" text="Usuario" />
            <Label fx:id="lblPassword" layoutX="23.0" layoutY="110.0" text="Contraseña" />
            <TextField fx:id="txtUsuario" layoutX="116.0" layoutY="63.0" />
            <TextField fx:id="txtPassword" layoutX="116.0" layoutY="106.0" />
            <Button fx:id="btnMostrarPassword" layoutX="241.0" layoutY="138.0" mnemonicParsing="false" onAction="#mostrarPassword" scaleX="2.0" />
            <Label fx:id="lblTitulo" layoutX="64.0" layoutY="23.0" scaleX="2.0" scaleY="2.0" text="Inicio de sesión" />
            <Button fx:id="btnMenu" layoutX="138.0" layoutY="151.0" mnemonicParsing="false" onAction="#alMenu" text="Menu" />
         </children>
      </Pane>
   </children>
</AnchorPane>
